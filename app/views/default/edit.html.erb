<div id="notice"></div>
<%= form_for instance_variable_get("@#{model_name}"), {:url=>_edit_path, :method=>"put"} do |f| %>
  
  <% model_class.columns.each do |c| %>
  <% next if ['id', 'created_at', 'updated_at'].include? c.name %>
  <%= f.label c.name %>
  <%= f.text_field c.name %><br />
  <% end %>
  
  <%= f.submit "Update", :id=>"submit" %>
<% end %>
<script>
  jQuery(document).ready(function(){
     group.init(); 
  });
</script>

